0.02;\n      case 'low': return 0.03;\n      default: return 0.02;\n    }\n  }\n\n  function update() {\n    const amount = Math.max(0, Number(amountEl.value || 0));\n    const term = Number(termEl.value || 0);\n    const rating = getRating();\n    const rate = getRate(rating);\n\n    rateBadge.textContent = 'Ставка: ' + (rate * 100).toFixed(2) + '% годовых';\n\n    if (amount <= 0 || term <= 0) {\n      totalFeeEl.textContent = '—';\n      monthlyFeeEl.textContent = '—';\n      summaryTermEl.textContent = '—';\n      summaryAmountEl.textContent = '—';\n      rateExplainEl.textContent = 'Выберите сумму и срок для расчета.';\n      return;\n    }\n\n    const totalFee = amount * rate * (term / 12);\n    const monthlyFee = totalFee / term;\n\n    totalFeeEl.textContent = nf.format(Math.max(0, Math.round(totalFee))) + ' ₽';\n    monthlyFeeEl.textContent = nf.format(Math.max(0, Math.round(monthlyFee))) + ' ₽';\n    summaryTermEl.textContent = term + ' мес.';\n    summaryAmountEl.textContent = nf.format(Math.round(amount)) + ' ₽';\n\n    let ratingText = 'высокий — 1.00%';\n    if (rating === 'medium') ratingText = 'средний — 2.00%';\n    if (rating === 'low') ratingText = 'низкий — 3.00%';\n    rateExplainEl.textContent = 'Ставка выбрана по профилю: ' + ratingText + '.';\n  }\n\n  form.addEventListener('input', update);\n  form.addEventListener('change', update);\n  form.addEventListener('submit', function(e) {\n    e.preventDefault();\n    update();\n    const section = document.getElementById('calc');\n    if (section && section.scrollIntoView) section.scrollIntoView({ behavior: 'smooth', block: 'center' });\n  });\n\n  resetBtn.addEventListener('click', function() {\n    amountEl.value = '';\n    termEl.value = '12';\n    const high = form.querySelector('input[name=\"rating\"][value=\"high\"]');\n    if (high) high.checked = true;\n    update();\n  });\n\n  update();\n})();\n"}]},{"type":"script","attributes":{"id":"i6vqjw3"},"components":[{"type":"textnode","content":"\n(function() {\n  const form = document.getElementById('leadForm');\n  const status = document.getElementById('formStatus');\n\n  function validateEmail(v) {\n    return /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/.test(v);\n  }\n  function validatePhone(v) {\n    return /[0-9]{7,}/.test((v || '').replace(/\\D/g, ''));\n  }\n\n  form.addEventListener('submit', function(e) {\n    e.preventDefault();\n    status.textContent = '';\n    const data = new FormData(form);\n    const name = (data.get('name') || '').toString().trim();\n    const phone = (data.get('phone') || '').toString().trim();\n    const email = (data.get('email') || '').toString().trim();\n    const consent = !!data.get('consent');\n\n    if (!name) { status.textContent = 'Укажите имя.'; return; }\n    if (!validatePhone(phone)) { status.textContent = 'Введите корректный телефон.'; return; }\n    if (!validateEmail(email)) { status.textContent = 'Введите корректный email.'; return; }\n    if (!consent) { status.textContent = 'Подтвердите согласие на обработку данных.'; return; }\n\n    status.textContent = 'Заявка отправляется...';\n\n    setTimeout(function() {\n      status.textContent = 'Спасибо! Ваша заявка принята, мы свяжемся с вами в ближайшее время.';\n      form.reset();\n    }, 800);\n  });\n})();\n"}]}],"doctype":"<!DOCTYPE html>","head":{"type":"head","attributes":{"id":"i1819p"},"components":[{"tagName":"meta","void":true,"attributes":{"charset":"utf-8","id":"itxrka"}},{"tagName":"meta","void":true,"attributes":{"name":"viewport","content":"width=device-width, initial-scale=1","id":"imnysg"}},{"tagName":"title","type":"text","attributes":{"id":"i1r71z"},"components":[{"type":"textnode","content":"Коммерческие банковские гарантии — расчет стоимости онлайн"}]},{"tagName":"link","void":true,"attributes":{"href":"https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600&family=Poppins:wght@600;700&display=swap","rel":"stylesheet","id":"ingvau"}}]},"docEl":{"tagName":"html","attributes":{"lang":"ru"}}},"id":"Qcn9B90Ds1WhJcmJ","minHeight":"500px"}],"id":"c0kYYHRCvGfvTPLrOR"}],"symbols":[],"custom":{"projectType":"web","id":""}}